    t\bar   % 4: \bar accented symbol
    m       % 5: symbol
    g     % 6: compartment
    o       % 7: superscript
    o       % 8: subscript
    }{%
    {\color{green!50!black}%
        \IfBooleanTF{#2}{\tilde#5}{%
        \IfBooleanTF{#3}{\hat  #5}{%
        \IfBooleanTF{#4}{\bar  #5}{%
                               #5}}%
        }\IfNoValueF{#7}{^{#7}}_{\IfNoValueF{#6}{\textsc{#6}}\IfNoValueF{#8}{\IfNoValueF{#6}{,}#8}}%
    }%
}
